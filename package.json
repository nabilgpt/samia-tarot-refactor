{
  "name": "samia-tarot-api",
  "version": "1.0.0",
  "description": "Complete backend API for SAMIA TAROT platform",
  "main": "src/api/index.js",
  "scripts": {
    "start": "node src/api/index.js",
    "dev": "nodemon src/api/index.js",
    "backend": "nodemon src/api/index.js",
    "frontend": "vite",
    "frontend:build": "vite build",
    "frontend:preview": "vite preview",
    "api": "nodemon src/api/index.js",
    "build": "npm run frontend:build",
    "test": "jest",
    "test:unit": "jest",
    "test:integration": "jest --testPathPattern=integration --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docs": "swagger-jsdoc -d swaggerDef.js src/api/*.js -o docs/swagger.json",
    "db:migrate": "node database/add_missing_foreign_keys.sql",
    "db:fix-columns": "echo 'Apply missing columns fix via Supabase Dashboard'",
    "db:seed": "node src/api/scripts/seed-data.js",
    "db:verify": "node src/api/scripts/pre-deployment-check.js",
    "db:complete-setup": "npm run db:fix-columns && npm run db:seed && npm run db:verify",
    "deploy:check": "npm run lint && npm run test && npm run db:verify",
    "deploy:prepare": "npm run db:seed && npm run deploy:check",
    "security:audit": "npm audit --audit-level=moderate",
    "qa:full": "node src/api/scripts/qa-report-generator.js",
    "qa:quick": "npm run db:verify && echo 'Quick QA check completed'",
    "health:check": "node src/api/scripts/quick-health-check.js",
    "launch:ready": "npm run health:check && npm run db:seed && npm run deploy:check",
    "server:health": "curl -f http://localhost:3000/health || exit 1",
    "cleanup:logs": "find logs -name '*.log' -mtime +7 -delete 2>/dev/null || true",
    "backup:db": "node src/api/scripts/backup-database.js",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "perf:audit": "lighthouse http://localhost:3000 --output=json --output-path=./performance-audit.json",
    "build:analyze": "npm run build -- --analyze"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.50.0",
    "@testing-library/dom": "^9.3.4",
    "@vitejs/plugin-react": "^4.5.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "framer-motion": "^12.16.0",
    "helmet": "^7.2.0",
    "i18next": "^25.2.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.513.0",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "openai": "^4.24.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-google-recaptcha": "^3.1.0",
    "react-i18next": "^15.5.2",
    "react-router-dom": "^7.6.2",
    "react-tsparticles": "^2.12.2",
    "recharts": "^2.15.3",
    "redis": "^4.6.10",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "square": "^43.0.0",
    "stripe": "^13.11.0",
    "tailwind-merge": "^3.3.0",
    "tsparticles-slim": "^2.12.0",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "vite": "^6.3.5",
    "web-vitals": "^3.4.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.54.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.4",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tailwindcss": "^3.4.17",
    "vitest": "^3.2.2",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "keywords": [
    "tarot",
    "api",
    "node.js",
    "express",
    "supabase",
    "payments",
    "booking",
    "chat"
  ],
  "author": "SAMIA TAROT Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samia-tarot/backend"
  }
}
